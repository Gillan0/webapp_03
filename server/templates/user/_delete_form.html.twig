{% comment %} <form method="post" action="{{ path('app_user_delete', {'id': user.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.id) }}">
    <button class="btn"><img src="{{ asset('ressources/images/delete.png') }}" id="delete" width ="10px" height ="10px"></button>
</form> {% endcomment %}
